<header id="top-header">
  <span class="siteName">
    <a href="/" title="Return to Home page">CSE Motors</a>
  </span>
  <div id="tools">
    <% if (locals.loggedin) { %>
    <a title="Click to view your account" href="/account"
      >Welcome, <b><%= locals.accountData.account_firstname %></b> |
      <a title="Click to logout" href="/account/logout">Logout</a></a
    >
    <% } else { %>
    <a title="Click to login" href="/account/login">My Account</a>
    <% } %>
  </div>
</header>
